                <category android:name="android.intent.category.DEFAULT"></category>

            </intent-filter>
        </service>

        <service android:name=".MyIntentServer"></service>


        <service
            android:name=".aidl.ServerAidl"
            android:process="com.aidl.NumberManageImpl">
            <intent-filter>
                <action android:name="numberManageImpl" />
                <category android:name="android.intent.category.DEFAULT"></category>

            </intent-filter>
        </service>

        <service android:name=".MyIntentServer"></service>
        <service android:name=".FloatingService"></service>


